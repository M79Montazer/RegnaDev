@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>رگنا</title>
    <style>
        body {
            margin: 0;
        }
    </style>
    <link href="~/lib/bootstrap/bootstrap.min.css" rel="stylesheet">

</head>
<body>
    @*<div style="height:10%; background-color: #f0f0f0;">Enemy: Imperial - Morale: 50 - Mana: 10</div>*@
    <div id="container" @*class="container-fluid"*@></div>
    @*<div style="height:10%; background-color: #f0f0f0">You: Mage - Morale: 50 - Mana: 10</div>*@

    <div class="modal fade" id="choosePlayerModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">choose your player</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    ...
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>



    <script type="importmap">
        {
          "imports": {
            "three": "/js/three.module.js",
            "three/addons/": "/js/jsm/"
          }
        }
    </script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/signalr/dist/browser/signalr.js"></script>
    <script src="~/lib/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="~/lib/tween/tween.umd.js"></script>
    <script src="~/lib/sweetalert2/sweetalert2.js"></script>
    @*<script src="~/js/Konva.js"></script>*@
    <script type="module" src="~/js/GamePlay/StartUp.js"></script>
    <script type="module" src="~/js/GamePlay/API.js"></script>
    <script type="module" src="~/js/GamePlay/Animate.js"></script>
    <script type="module" src="~/js/GamePlay/GamePlay_three.js"></script>
</body>
</html>
